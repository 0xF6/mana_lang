#space "mana/lang"

[special, forwarded]
public class Array
{
    [native("!!value")]
    private _value: ValueType;

    [native("!!block")]
    private _block: Int64;

    [native("!!size")]
    public readonly Length: Int64;

    [native("!!rank")]
    public readonly Rank: Int64;

    public override toString(): String
    {
        return "Array";
    }

    protected extern _indexer_get(index: Int64): Object;
    protected extern _indexer_set(index: Int64, o: Object): Void;
}