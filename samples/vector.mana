#use "stl.lib"

struct Vector3D 
{
    public x: float;
    public y: float;
    public z: float;

    public new()
        => base(0f, 0f, 0f);
    public new(x: float)
        => base(x, 0f, 0f);
    public new(x: float, y: float)
        => base(x, y, 0f);
    public new(x: float, y: float, z: float)
        => this = (x, y, z);
}