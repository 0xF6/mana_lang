    <PackageReference Include="xunit.assert" Version="2.8.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\lib\spectre.console\src\Spectre.Console.Cli\Spectre.Console.Cli.csproj" />
